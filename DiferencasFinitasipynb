   "source": [
    "# f(x) = x² + 4x + 4\n",
    "# f'(x) = 2x + 4 Derivada Exata \n",
    "\n",
    "def f (x) :\n",
    "    return np.exp(x)*np.sin(x) + np.exp(x)*np.cos(x) #2*x + 4\n",
    "\n",
    "def g (x) :\n",
    "    return np.exp(x)*np.sin(x) #x**2 + 4*x + 4\n",
    "\n",
    "hh = np.array([0.1, 0.01, 0.0001])\n",
    "\n",
    "xx = np.linspace(-2, 2, 100)\n",
    "\n",
    "for h in hh:\n",
    "    primeiro_ponto = True\n",
    "    for i in range(len(xx)):\n",
    "        x = xx[i] \n",
    "        dfp = (g(x+h) - g(x))/h\n",
    "        dfc = (g(x+h) - g(x-h))/(2*h)\n",
    "        erro1 = abs(dfp - f(i))\n",
    "        erro2 = abs(dfc - f(i))\n",
    "        if primeiro_ponto:\n",
    "            plt.scatter(x, dfp, color='red', label=\"Derivada progressiva\", s=50, edgecolor='black', marker = \"^\")\n",
    "            plt.scatter(x , dfc , color='blue', label=\"Derivadas central\", s=50, edgecolor='black', marker = \"D\")\n",
    "            primeiro_ponto = False\n",
    "            \n",
    "        else:\n",
    "            plt.scatter(x , dfp, color='red', s=50, edgecolor='black', marker = \"^\")\n",
    "            plt.scatter(x , dfc, color='blue', s=50, edgecolor='black', marker = \"D\")\n",
    "        print(\"Erro da derivada progressiva:\",erro1,\"ponto:\",i)\n",
    "        print(\"Erro da derivada central:\",erro2,\"ponto:\",i)\n",
    "\n",
    "\n",
    "    #x = np.linspace (-10,10,200) \n",
    "    plt.plot(xx,f(xx),color = \"black\", label = \"Derivada exata de f\")\n",
    "    plt.title(\"Derivada de f(x) por aproximação com h muito próximo de zero\")\n",
    "    plt.xlabel(\"x\")\n",
    "    plt.ylabel(\"y\")\n",
    "    plt.grid(True)\n",
    "    plt.legend() \n",
    "    plt.show()\n"
   ]
  },
